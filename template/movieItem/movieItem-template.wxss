@import '/template/star/star-template.wxss';

.movieList_movieWrap {
    margin-left: -15px;
    overflow: hidden;
}


.movieItem_container {
    float: left;
    box-sizing: border-box;
    width: 33.33%;
    padding: 0 0 14px 15px;
}

.movieItem_img {
    width: 100%;
    height: 290rpx;
}

.movieItem_name {
    display: block;
    width: 100%;
    font-size: 14px;
    color: #7c7c7c;
    line-height: 1;
    margin: 12px 0;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}